{
    "name": "chat.dev-consumer",
    "version": "1.0.6",
    "description": "Connect to, and interact with, chat.dev",
    "main": "index.js",
    "types": "index.d.ts",
    "type": "module",
    "scripts": {
        "test": "npm run test",
        "clean": "rm -rf ./lib",
        "build": "npm run clean && npm run build:esm && npm run build:ts",
        "build:esm": "tsc -p ./tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
        "build:cjs": "tsc -p ./tsconfig.cjs.json",
        "build:ts": "tsc -p ./tsconfig.json",
        "prepack": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/extensional/chat.dev-consumer.git"
    },
    "keywords": [
        "chat.dev",
        "chat",
        "anarchy",
        "llm",
        "npl",
        "openai"
    ],
    "author": "Nico Kupfer @ Anarchy.ai",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/extensional/chat.dev-consumer/issues"
    },
    "homepage": "https://github.com/extensional/chat.dev-consumer#readme",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.46.1",
        "eslint": "^8.32.0",
        "eslint-config-standard-with-typescript": "^31.0.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^15.6.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.32.1",
        "eslint-plugin-unused-imports": "^2.0.0",
        "typescript": "^4.9.5"
    },
    "files": [
        "lib/**/*",
        "@types"
    ],
    "dependencies": {
        "axios": "^1.3.4",
        "chat.dev-config": "^1.0.5",
        "rxjs": "^7.8.0"
    },
    "exports": {
        ".": {
            "import": {
                "types": "./lib/esm/types/index.d.ts",
                "default": "./lib/esm/index.mjs"
            },
            "require": {
                "types": "./lib/cjs/types/index.d.ts",
                "default": "./lib/cjs/index.js"
            }
        }
    }
}
